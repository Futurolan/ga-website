{% set heading_id = attributes.id ~ '-menu'|clean_id %}
<div id="block-multisite">
    <div class="container">
        <a href="http://ga.gamers-assembly.net" class="active gamer-assembly"><span class="hidden-sm hidden-xs">GAMERS ASSEMBLY</span><span class="hidden-md hidden-lg">GA</span></a>
        <a href="http://halloween.gamers-assembly.net"class="galloween"><span class="hidden-sm hidden-xs">GAMERS ASSEMBLY HALLOWEEN EDITION</span><span class="hidden-md hidden-lg">GAHE</span></a>
        <a href="http://presse.futurolan.net" target="_blank" class="futurolan"><span class="hidden-sm hidden-xs">ASSOCIATION 
FUTUROLAN</span><span class="hidden-md hidden-lg">FUTUROLAN</span></a>
    </div>
</div>
<nav class="navbar navbar-default navbar-fixed-top" role="navigation"
     aria-labelledby="{{ heading_id }}"{{ attributes|without('role', 'aria-labelledby') }}>
    <div class="container">
        <div class="navbar-header">
            <button type="button" class="navbar-toggle collapsed" data-toggle="collapse"
                    data-target="#ga-navbar-collapse" onClick="jQuery('body').toggleClass('mobile-menu-open');" aria-expanded="false">
                <span class="sr-only">Toggle navigation</span>
                <span class="icon-bar"></span>
                <span class="icon-bar"></span>
                <span class="icon-bar"></span>
            </button>
            <a class="navbar-brand" href="{{ path('<front>') }}" title="{{ site_name }}" rel="home">
            </a>
        </div>

        <div class="collapse navbar-collapse" id="ga-navbar-collapse">

            {# Label. If not displayed, we still provide it for screen readers. #}
            {% if not configuration.label_display %}
                {% set title_attributes = title_attributes.addClass('visually-hidden') %}
            {% endif %}
            <!--<h2{{ title_attributes }}>{{ configuration.label }}</h2>-->
            {% block content %}
                {{ content }}
                {% if(config.main_sponsor_image) %}
                    <ul class="nav navbar-nav pull-right" id="menu-right">
                        <li>
                            <a href="{{ config.main_sponsor_url}}" class="main-sponsor"><img src="{{ file_url(config.main_sponsor_image) }}"
                                                                  class="img-responsive"/></a>
                        </li>
                    </ul>
                {% endif %}
            {% endblock %}
        </div>

    </div>

</nav>
