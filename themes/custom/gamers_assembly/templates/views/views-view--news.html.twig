{% set classes = [
dom_id ? 'js-view-dom-id-' ~ dom_id,
] %}
<div class="views-news">
    <div {{ attributes.addClass(classes) }}>
        <div class="grey-area-title">
            {% include directory ~ "/components/title-line/title-line.html.twig" with {
            title:"Last news"|t,
            } only %}
        </div>
        <div class="white-area">
            <div class="container">
                {{ rows }}
                {{ empty }}
                {{ pager }}

                {{ attachment_after }}
                {{ more }}
            </div>
        </div>
    </div>
</div>

