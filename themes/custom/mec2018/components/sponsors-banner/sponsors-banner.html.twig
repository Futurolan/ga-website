{{ attach_library('mec2018/mec2018.sponsors-banner') }}

<div class="slideshow">
    <div class="footer-slider-container">
        {% for sponsorList in sponsors %}
            {% if(sponsorList.displayFront) %}
                {% for sponsor in sponsorList.sponsors %}
                    <figure>
                        <img src="{{ file_url(sponsor.image) }}" alt="" class="img-responsive center-block">
                    </figure>
                {% endfor %}
            {% endif %}
        {% endfor %}
    </div>
</div>
