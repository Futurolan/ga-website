{#
/**
 * @file stream.html.twig
 * Default theme implementation to present Stream data.
 *
 * This template is used when viewing Stream pages.
 *
 *
 * Available variables:
 * - content: A list of content items. Use 'content' to print all content, or
 * - attributes: HTML attributes for the container element.
 *
 * @see template_preprocess_stream()
 *
 * @ingroup themeable
 */
#}
<div{{ attributes.addClass('stream') }}>
  {% if content %}
    {{- content -}}
  {% endif %}
</div>
